{
  "name": "create-react-app-lite",
  "version": "1.2.0",
  "description": "A React Boilerplate from scratch using Javascript, Webpack, ESLint, Prettier, and Jest",
  "bin": "./bin/cli.js",
  "scripts": {
    "start": "react-scripts-lite start",
    "build": "react-scripts-lite build",
    "test": "react-scripts-lite test",
    "lint": "eslint -c config/eslint/.eslintrc.js --ignore-path config/eslint/.eslintignore --fix .",
    "prettier": "prettier --config config/prettier/.prettierrc.js --ignore-path config/prettier/.prettierignore --write .",
    "prepare": "husky install"
  },
  "author": "eggfreitag",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eggfreitag/create-react-app-lite.git"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@testing-library/react": "^13.3.0",
    "babel-loader": "^8.2.5",
    "chalk": "^4.1.2",
    "css-loader": "^6.7.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.0",
    "jest": "^28.1.2",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "react-scripts-lite": "^1.0.5",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.5"
  }
}
