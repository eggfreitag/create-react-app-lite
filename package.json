{
  "name": "create-react-app-lite",
  "version": "1.0.5",
  "description": "A React Boilerplate from scratch using Javascript, Webpack, ESLint, Prettier, and Jest",
  "bin": "./bin/cli.js",
  "scripts": {
    "start": "react-scripts-lite start",
    "build": "react-scripts-lite build",
    "lint": "eslint -c config/eslint/.eslintrc.js --ignore-path config/eslint/.eslintignore --fix .",
    "prettier": "prettier --config config/prettier/.prettierrc.js --ignore-path config/prettier/.prettierignore --write .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eggfreitag/create-react-app-lite.git"
  },
  "keywords": [
    "create-react-app",
    "react-boilerplate",
    "react"
  ],
  "author": "eggfreitag",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eggfreitag/create-react-app-lite/issues"
  },
  "homepage": "https://github.com/eggfreitag/create-react-app-lite#readme",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/react": "^13.3.0",
    "babel-loader": "^8.2.5",
    "chalk": "^4.1.2",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.1",
    "dotenv-webpack": "^8.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "fs": "^0.0.1-security",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.0",
    "jest": "^28.1.2",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss-loader": "^7.0.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "react-scripts-lite": "^1.0.3",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.5",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
